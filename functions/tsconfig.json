{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "./lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "rootDir": "src"
  },
  "compileOnSave": true,
  "include": [
    "./src"
  ],
  "exclude": [
    "node_modules",
    "./node_modules",
    "../src-nonono",
    "../node_modules",
    "../node_modules/@types/",
    "../public",
    "./lib"
  ]
}
